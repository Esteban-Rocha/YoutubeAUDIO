{

  "manifest_version": 2,
  "name": "YoutubeAUDIO",
  "version": "1.2",
  "homepage_url": "https://github.com/sheddup/YoutubeAUDIO",
  "description": "Listen to Youtube, no video data downloaded. Embeds not supported.",
  "author": "sheddup",

  "applications": {
    "gecko": {
       "id": "{9be80948-e099-438b-8246-d1b679c89d47}"
     }
  },

  "icons": {
    "48": "icons/extensionLogo.png"
  },

  "permissions": [
    "activeTab",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*.youtube.com/*",
    "*://*.googlevideo.com/*"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": {
      "16": "icons/ytaudioOFF32.png",
      "32": "icons/ytaudioOFF32.png"
    },
    "default_title": "YoutubeAUDIO Disabled"
  }

}
